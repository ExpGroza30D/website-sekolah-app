{
    "version": 2,
    "framework": "laravel",
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://website-sekolah-chi.vercel.app",
      "ASSET_URL": "https://website-sekolah-chi.vercel.app"
    }
  }